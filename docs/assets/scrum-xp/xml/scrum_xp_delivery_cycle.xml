        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_SelectUserStories_di" bpmnElement="Task_SelectUserStories">
        <dc:Bounds x="820" y="390" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      

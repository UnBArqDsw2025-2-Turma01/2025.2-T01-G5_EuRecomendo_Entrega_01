      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_DefineProductVision_di" bpmnElement="Task_DefineProductVision">
        <dc:Bounds x="340" y="100" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_CreateProductBacklog_di" bpmnElement="Task_CreateProductBacklog">
        <dc:Bounds x="500" y="100" width="100" height="80" />
        <bpmndi:BPMNLabel />
